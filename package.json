{
  "name": "maplibre-gl-shortbread-styler",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && npm run compile && rm -rf dist/dts",
    "compile": "rollup -c",
    "dev": "trap 'kill 0' SIGINT; npm run watch & http-server -sc0",
    "lint": "eslint",
    "upgrade": "npm-check-updates -u && rm -f package-lock.json && rm -rf node_modules; npm i",
    "watch": "rollup -cw"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "eslint": "^8.57.0",
    "http-server": "^14.1.1",
    "npm-check-updates": "^16.14.15",
    "rollup": "^4.12.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-less": "^1.1.3",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.1.0"
  },
  "dependencies": {
    "@versatiles/style": "^4.2.4",
    "maplibre-gl": "^4.1.0"
  }
}
